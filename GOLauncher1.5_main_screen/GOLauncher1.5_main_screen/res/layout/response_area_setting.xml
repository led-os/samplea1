<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/go_lock_widegt_area_setting"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#cc000000" >
    
    <View
        android:id="@+id/go_lock_widegt_area_move2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_widget_area_right" />

    <View
        android:id="@+id/go_lock_widegt_area_move"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_widget_area_left" />

    <ImageView
        android:id="@+id/go_lock_widegt_area_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_widget_area_top" />

    <ImageView
        android:id="@+id/go_lock_widegt_area_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_widget_area_center" />

    <ImageView
        android:id="@+id/go_lock_widegt_area_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_widget_area_bottom" />
    
</FrameLayout>