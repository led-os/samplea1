<?xml version="1.0" encoding="utf-8"?>
<com.jiubang.ggheart.appgame.appcenter.appmigration.AppMigrationContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="#FFFFFF">

    <LinearLayout
        android:id="@+id/app_migration_tips_view"
        android:background="#FFFFFF"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </LinearLayout>

    <com.jiubang.ggheart.appgame.appcenter.component.PinnedHeaderListView
        android:id="@+id/app_migration_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/center_background"
        android:dividerHeight="0.5dp"
        android:visibility="gone"
        android:cacheColorHint="#00000000"
        android:fadingEdge="none"
        android:footerDividersEnabled="true"
        android:scrollbarStyle="outsideOverlay"
        android:listSelector="@android:color/transparent"
        android:divider="@drawable/listview_divider">
    </com.jiubang.ggheart.appgame.appcenter.component.PinnedHeaderListView>
	
</com.jiubang.ggheart.appgame.appcenter.appmigration.AppMigrationContainer>